					<a onclick="genera_codi()" class="boto_verd" style="float: right">Genera el codi</a>
					<script type="text/javascript">
						function genera_codi()
						{
							error = false;
							mida = $('#ipt-mida').val();

							if( !isInt( mida ) )
							{
								alert( "No has escrit un número a l'ampalda" );
								error = true;
							}
							else if( mida < 200 )
							{
								alert( "L'amplada ha de ser més gran que 200" );
								error = true;
							}
							
							id_lletra = $('#nom_id_lletra').val();
							color = $('#ipt-color').val().replace('#','');
							tipus = $('input[name=ipt-tipus]:checked').val()
							
							if( !tipus )
								tipus = 1;

							if( !error )
							{
								$('#ipt-codi').slideDown();
								$('#ipt-codi').val( '<iframe width="'+ mida +'" height="auto" frameborder="0" allowfullscreen="" src="http://viasona.cat/iframe/'+ id_lletra +'/'+ tipus +'/'+ mida +'/'+ color +'"></iframe>' );
							}
						}
						
						function isInt(x) {
						  var y=parseInt(x);
						  if (isNaN(y)) return false;
						  return x==y && x.toString()==y.toString();
						} 
					</script>
